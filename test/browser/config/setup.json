{
	"packages": {
		"test": {
			"application": "./public/application.js",
			"base": "data",
			"modules": [
				"./app/*",
				"./test/*",
				"./setup.js",
				"events",
				"./libs/jquery.js"
			],
			"run": [
				"- ./libs/one.js",
				"/app/Two",
				"- ./libs/three.js",
				"/test/Require",
				"/test/Libraries",
				"/test/Styles"
			],
			"libraries": {
				"begin": [
					"./libs/prepare.js"
				]
			},
			"aliases": {
				"app": "/app/Application"
			},
			"style": {
				"in": "./styles/style.less",
				"out": "./public/style.css"
			}
		}
	}
}