{
	"name": "simq",
	"description": "Simple require for javascript (browser)",
	"version": "5.6.0",
	"author": {
		"name": "David Kudera",
		"email": "sakren@gmail.com"
	},
	"keywords": [
		"require",
		"client",
		"minify",
		"compressor",
		"commonjs",
		"server",
		"browser"
	],
	"repository": {
		"type": "git",
		"url": "git@github.com:sakren/node-simq.git"
	},
	"license": "MIT",
	"engines": {
		"node": ">=0.9"
	},
	"main": "./lib/SimQ.js",
	"bin": {
		"simq": "./bin/simq"
	},
	"dependencies": {
		"watch": "~0.8.0",
		"uglify-js": "~2.4.10",
		"clean-css": "~2.0.7",
		"optimist": "~0.6.0",
		"ncp": "~0.4.2",
		"q": "~1.0.0",
		"express": "~3.4.8",
		"mime": "~1.2.11",
		"easy-configuration": "~2.0.0",
		"fs-finder": "~1.8.0",
		"source-compiler": "~2.1.1",
		"recursive-merge": "~1.2.0",
		"flatten-required": "~1.1.1",
		"module-info": "~1.2.2",
		"escape-regexp": "~0.0.1"
	},
	"devDependencies": {
		"chai": "~1.8.1",
		"rimraf": "~2.2.6",
		"mocha": "~1.17.0",
		"mocha-phantomjs": "~3.3.1",
		"phantomjs": "~1.9.2-6"
	},
	"scripts": {
		"pretest": "node ./test/browser/prepare.js",
		"test": "npm run test-node && npm run test-browser",
		"test-node": "mocha ./test/node/index.js --reporter spec --timeout 10000",
		"test-browser": "mocha-phantomjs -p ./node_modules/phantomjs/bin/phantomjs ./test/browser/index.html"
	}
}